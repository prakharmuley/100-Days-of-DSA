Problem Link: https://leetcode.com/problems/count-operations-to-obtain-zero/



class Solution {
    public int countOperations(int num1, int num2) {
        int c=0;
        if(num1==0||num2==0) return 0;
        while(true)
        {
            if(num1>=num2) {num1-=num2; c++;}
            else {num2-=num1; c++;}
            
            if(num1==0||num2==0) break;
            
            
            
        }
        return c;
    }
}
